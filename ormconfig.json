{
  "name": "default",
  "type": "sqlite",
  "database": "db.sqlite3",
  "autoSchemaSync": true,
  "entities": ["src/entity/*.ts"],
  "migrations": ["src/migration/*.ts"],
  "synchronize": true,
  "logging": {
    "logQueries": true
  },
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration"
  }
}
